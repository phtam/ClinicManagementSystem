@model ClinicManagementSystem.EF.EducationFeedback

@{
    ViewBag.Title = "Details EducationFeedback";
    ViewBag.Function = "EducationFeedbacks";
    ViewBag.ChildFunction = "EducationFeedback Details";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}
}


<section class="content">
    <div class="card">
        <div class="card-header">
            <h1 style="font-weight:bold; font-size:25px;" class="card-title">Detail of EducationFeedback</h1>

            <div class="card-tools">
                <a class="btn btn-block btn-secondary float-right" href="~/Admin/EducationFeedBacks">Back to List</a>
            </div>
        </div>
        <div class="card-body">
            <div class="row">
                <div class="col-12 col-md-12 col-lg-12 order-12 order-md-12">
                    <div class="row">
                        <div class="col-12">
                            <div class="post">
                                <div class="col-sm-2 user-block">
                                    <strong> @Html.DisplayNameFor(model => model.Content)</strong>:
                                </div>
                                <p class="col-sm-10">
                                    @Html.DisplayFor(model => model.Content)
                                </p>
                            </div>
                            <div class="post">
                                <div class="col-sm-2 user-block">
                                    <strong>@Html.DisplayNameFor(model => model.CreatedDate)</strong>:
                                </div>
                                <p class="col-sm-10">
                                    @Html.DisplayFor(model => model.CreatedDate)

                                </p>
                            </div>
                            <div class="post">
                                <div class="col-sm-2 user-block">
                                    <strong> @Html.DisplayNameFor(model => model.Customer.Password)</strong>:
                                </div>
                                <p class="col-sm-10">
                                    @Html.DisplayFor(model => model.Customer.Password)

                                </p>
                            </div>
                            <div class="post">
                                <div class="col-sm-2 user-block">
                                    <strong>@Html.DisplayNameFor(model => model.Education.LessonName)</strong>:
                                </div>
                                <p class="col-sm-10">
                                    @Html.DisplayFor(model => model.Education.LessonName)
                                </p>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </div>
</section>
